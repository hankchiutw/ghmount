{
  "name": "ghmount",
  "description": "Mount github repo in filesystem",
  "version": "0.1.0",
  "author": "hankchiutw",
  "bin": {
    "ghmount": "bin/run"
  },
  "scripts": {
    "test": "mocha",
    "build": "rm -rf lib && tsc -b && oclif-dev readme",
    "bump": "HUSKY=0 release-it --ci",
    "dev": "./bin/run",
    "lint": "yarn lint:src",
    "lint:src": "tsc --noEmit && eslint './src/**/*.ts'",
    "lint:fix": "yarn lint:src --fix && pretty-quick --staged",
    "version": "oclif-dev readme && git add README.md",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "bugs": "https://github.com/hankchiutw/ghmount/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "execa": "^5.0.0",
    "fuse-native": "^2.2.6",
    "inquirer": "^8.0.0",
    "inversify": "^5.1.1",
    "inversify-inject-decorators": "^3.1.0",
    "kleur": "^4.1.4",
    "octokit": "^1.0.6",
    "ora": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/chai": "^4.2.16",
    "@types/fs-extra": "^9.0.8",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "release-it": "^14.4.1",
    "ts-node": "^8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/hankchiutw/ghmount",
  "keywords": [
    "chrome",
    "esm"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "ghmount"
  },
  "repository": "hankchiutw/ghmount",
  "types": "lib/index.d.ts"
}
